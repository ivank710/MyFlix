json.lists do 
  @lists.each do |list_item|
    json.set! list_item.id do
      json.extract! list_item, :id, :user_id, :movie_id
    end
  end
end

json.movies do 
  @movies.each do |movie|
    json.set! movie.id do
      json.extract! movie, :id, :title, :description, :year, :genre
    end
  end
end